
<%- include('../partials/adminHeader.ejs') %>

            <!-- Table Start -->

            <div class="container-fluid pt-4 px-4">
                <div class="row g-4">
                    <div class="col-12">
                        <div class="bg-light rounded h-100 p-4">
                            <h6 class="mb-4">Catogarys</h6>  
                            
                              <a href="addcatogory" class='btn btn-success '>Add</a>
                            <div class="table-responsive">
                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th scope="col">No</th>
                                            <th scope="col">Catogorys</th>
                                            <th scope="col"></th>
                                            <th scope="col"></th>
                                            <th scope="col"></th>
                                            <th scope="col">Edit</th>
                                            <th scope="col">Status</th>
                                            <!-- <th scope="col">Status</th> -->
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <%for(let i=0;i<catogoryData.length;i++){%>
                                        <tr>
                                           
                                            <th scope="row"><%=i+1%></th>
                                            <td><%=catogoryData[i].name%></td>
                                            <td></td>
                                            <td></td>
                                            <td></td>
                                            <td><a href="/admin/editcatogory?name=<%=catogoryData[i].name%>"><img src="/admin/img/edit.png" alt=""></a></td>
                                            <%if(catogoryData[i].Block!==true){%>
                                            <td><button class="btn btn-success btn-sm"><a href="/admin/block?id=<%=catogoryData[i]._id%>&&name=<%=catogoryData[i].name%>" style="color: #fff;">List</a></button></td>
                                            <%}else{%>
                                            <td><button class="btn btn-danger btn-sm"><a href="/admin/block?id=<%=catogoryData[i]._id%>&data='hai'&&name=<%=catogoryData[i].name%>" style="color: #fff;">Unlist</a></button></td>
                                            <%}%>
                                            <!-- <td>123</td>
                                            <td>Member</td> -->
                                        </tr>
                                        <%}%>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Table End -->


 <%- include('../partials/adminFooter.ejs') %>